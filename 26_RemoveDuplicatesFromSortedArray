/*********************************************************************************
  *FileName:26_RemoveDuplicatesFromSortedArray 
  *Author:  Javan_Z
  *Version: 1.0
  *Date:  2016.5.29
  36ms, beats 26.35%ï¼ŒUse Two Pointer to Swap 
**********************************************************************************/
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int len=nums.size();
        if(!len) return 0;
        int left=0,right=1;
        while(right<len){
            if(nums[right]>nums[left])
              swap(nums,++left,right);
            right++;
        }
        return left+1;
    }
    void swap(vector<int>& nums,int a,int b){
        int tmp;
        tmp=nums[a];
        nums[a]=nums[b];
        nums[b]=tmp;
    }
};
